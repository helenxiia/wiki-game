{
    "manifest_version": 2,
    "name": "wikipedia link race game",
    "version": "1.0",
    "description": "automatically generates a start and end site, user needs to navigate it.",
    "permissions": [
        "activeTab", // For the current tab 
        "storage", // For the tabs clicked
        "declarativeContent",
        "tabs" 
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action" : { 
        "default_icon" : "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128":"images/icon.png"
    }
}